<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".order">

  <com.google.android.material.appbar.AppBarLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:background="@android:color/white"
      android:orientation="vertical"
      app:elevation="0dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toTopOf="parent" >


    <androidx.appcompat.widget.Toolbar
        android:id="@+id/tbToolbar"
        android:layout_width="match_parent"
        android:layout_height="96.71dp"
        app:navigationIcon="@drawable/ic_back"
        app:buttonGravity="center_vertical">

      <RelativeLayout
          android:layout_width="138dp"
          android:layout_height="22dp"
          android:layout_gravity="center">

        <TextView
            android:id="@+id/complete_bo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal|top"
            android:text="Your Ticket"
            android:textAppearance="@style/nu_exb_16"
            android:textColor="#3D3D3D"/>

      </RelativeLayout>

    </androidx.appcompat.widget.Toolbar>

  </com.google.android.material.appbar.AppBarLayout>

  <androidx.constraintlayout.widget.ConstraintLayout
      android:layout_width="342dp"
      android:layout_height="150dp"
      android:layout_alignParentLeft="true"
      android:layout_alignParentTop="true"
      android:layout_marginStart="32dp"
      android:layout_marginTop="112dp"
      android:background="@color/white"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent">

    <View
        android:id="@+id/rectangle"
        android:layout_width="342dp"
        android:layout_height="39dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/ticketbar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="0dp"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="123dp"
        android:layout_height="23dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="12dp"
        android:gravity="top"
        android:lineSpacingExtra="10sp"
        android:text="23 JUL 2019. 17:35"
        android:textColor="@color/white"
        android:textStyle="bold"
        android:translationY="-4.8sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/book_no_230"
        android:layout_width="150dp"
        android:layout_height="23dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="176dp"
        android:layout_marginTop="12dp"
        android:gravity="top"
        android:lineSpacingExtra="10sp"
        android:text="Book No. 230719-0001"
        android:textColor="@color/white"
        android:textStyle="bold"
        android:translationY="-4.8sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/sempati_sta"
        android:layout_width="99dp"
        android:layout_height="22dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="28dp"
        android:layout_marginTop="48dp"
        android:gravity="top"
        android:text="@string/po_name"
        android:textAppearance="@style/nu_exb_16"
        android:textColor="@color/red"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/p_9123_nn2"
        android:layout_width="80dp"
        android:layout_height="22dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="144dp"
        android:layout_marginTop="48dp"
        android:gravity="top"
        android:text="P 1923 NM"
        android:textColor="@color/grey"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/time_arrival2"
        android:layout_width="38dp"
        android:layout_height="19dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="28dp"
        android:layout_marginTop="84dp"
        android:gravity="top"
        android:text="@string/time_arrival"
        android:textAppearance="@style/nu_exb_14"
        android:textColor="#585858"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:layout_width="22dp"
        android:layout_height="21dp"
        android:layout_marginStart="232dp"
        android:layout_marginTop="84dp"
        android:background="@drawable/done"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/time_arrival3"
        android:layout_width="38dp"
        android:layout_height="19dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="264dp"
        android:layout_marginTop="84dp"
        android:gravity="top"
        android:text="Paid"
        android:textColor="@color/red"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/terminal_am2"
        android:layout_width="91dp"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="84dp"
        android:layout_marginTop="84dp"
        android:gravity="top"
        android:lineSpacingExtra="10sp"
        android:text="Terminal Amplas Medan"
        android:textAppearance="@style/lato_12"
        android:textColor="#D8D8D8"
        android:translationY="-4.8sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


  </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>


